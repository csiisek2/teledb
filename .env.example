# TeleDB 환경변수 예시 파일
# 실제 사용시 .env 파일을 만들어서 값을 설정하세요

# 텔레그램 봇 토큰 (BotFather에서 발급)
BOT_TOKEN=your_telegram_bot_token_here

# SQLite 데이터베이스 파일 경로
DATABASE_PATH=./teledb.sqlite

# 관리자 텔레그램 사용자 ID
# 자신의 텔레그램 사용자 ID를 확인하려면 @userinfobot에게 /start를 보내세요
ADMIN_USER_ID=123456789

# 디버그 모드 (개발시 true, 운영시 false)
DEBUG=true

# 로그 레벨 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# PostgreSQL 연결 (Railway 배포시 자동 설정됨)
# DATABASE_URL=postgresql://user:password@host:port/database